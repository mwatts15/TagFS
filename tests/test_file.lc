#include "test.h"
#include "util.h"
#include "file.h"

%(test AbstractFile create)
{
    AbstractFile a;
    abstract_file_init(&a, "aFile");
    char buffer[MAX_FILE_NAME_LENGTH];
    CU_ASSERT_EQUAL(0, get_file_id(&a));
    CU_ASSERT_STRING_EQUAL(file_to_string(&a, buffer), "0:aFile");
}

%(test File new)
{
    File *f = new_file("afile");
    file_destroy(f);
}

int main ()
{
    %(run_tests)
}

