#include <stdio.h>
#include "fc_cache.h"
#include "test.h"
#include "util.h"

static FCCache _cache;
static FCCache *cache = &_cache;

%(setup FCCache)
{
    fc_cache_init(&_cache);
}

%(teardown FCCache)
{
    fc_cache_destroy(cache);
}

%(test FCCache test_invalidate)
{
    fc_cache_insert(cache, 23, "hello", 0xdeadbeef);
    fc_cache_invalidate(cache, 23);
}

int main ()
{
   %(run_tests);
}


